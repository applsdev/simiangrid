php_version: '5.3.6'

php_extensions:
    - curl
    - gd
    - mysql
    - xmlrpc

document_root: /

shared_writable_dirs: [
    /Grid/config,
    /Grid/logs, 
    /GridFrontend/application/config,
    /GridFrontend/system/logs
]
